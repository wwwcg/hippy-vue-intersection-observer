{
  "name": "my-test-intersection-observer",
  "version": "0.0.7",
  "description": "IntersectionObserver for hippy-vue",
  "author": "wwwcg",
  "scripts": {
    "build": "rollup --config rollup.config.js",
    "lint": "eslint ./src",
    "build:version": "yarn version --patch --message \"chore(release): publish v%s\"",
    "dev": "rollup --watch --config rollup.config.js",
    "lint:fix": "eslint --fix ./src",
    "prepare": "npx husky install",
    "pub": "yarn build && yarn build:version && npm publish"
  },
  "main": "dist/index.js",
  "dependencies": {
    "core-js": "^3.8.3",
    "lodash": "^4.17.21",
    "vue": "^3.2.37"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/eslint-parser": "^7.17.0",
    "@vue/compiler-sfc": "^3.2.31",
    "babel-core": "^6.26.3",
    "babel-eslint-parser": "^7.13.10",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-3": "^6.24.1",
    "core-js": "^3.21.1",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-config-tencent": "^1.0.3",
    "eslint-config-vue": "^2.0.2",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.5.0",
    "husky": "4",
    "lint-staged": "^12.3.4",
    "prettier": "^2.5.1",
    "rollup-plugin-babel": "3.0.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-vue": "^6.0.0"
  }
}
